/****** Object:  Database [MADSKOOL]    Script Date: 11/06/2015 20:19:17 ******/
CREATE DATABASE [MADSKOOL] ON  PRIMARY 
( NAME = N'MADSKOOL', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL.1\MSSQL\DATA\MADSKOOL.mdf' , SIZE = 3072KB , MAXSIZE = UNLIMITED, FILEGROWTH = 1024KB )
 LOG ON 
( NAME = N'MADSKOOL_log', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL.1\MSSQL\DATA\MADSKOOL_log.ldf' , SIZE = 1024KB , MAXSIZE = 2048GB , FILEGROWTH = 10%)
GO
EXEC dbo.sp_dbcmptlevel @dbname=N'MADSKOOL', @new_cmptlevel=90
GO
IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [MADSKOOL].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO
